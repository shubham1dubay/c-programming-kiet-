import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int num;
	    System.out.println("Enter the number");
	    Scanner sc=new Scanner(System.in);
	    num=sc.nextInt();
	    int temp=num;
	    int rev=0,rem;
		while(temp!=0)
		{
		    rem=temp%10;
		    rev=rev*10+rem;
		    temp=temp/10;
		}
		if(num==rev)
		{
		    System.out.println(num+"Number is palindrome");
		
		}   else{
		    
		    System.out.println(num+"Numbre is not palindrome");
		}
	}
}
